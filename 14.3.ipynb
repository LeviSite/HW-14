   "source": [
    "import pandas as pd\n",
    "from scipy.stats import spearmanr\n",
    "\n",
    "df = pd.read_csv(\"healthy_breast.tsv\", sep = \"\\t\", index_col = 0)\n",
    "\n",
    "l = []\n",
    "for i in df.index:\n",
    "    if spearmanr(df.loc[\"SPI1\"], df.loc[i], axis=1)[0] >= 0.8:\n",
    "        l.append(i)\n",
    "print(l)"
